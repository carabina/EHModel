#IndentWrappedFunctionNames: true
#IndentFunctionDeclarationAfterType: true
#JSQS_Leave JSQS_Single JSQS_Double
#JavaScriptQuotes: Single
#KeepEmptyLinesAtTheStartOfBlocks: true
#MacroBlockBegin:
#MacroBlockEnd:
