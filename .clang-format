#LS_Cpp03 LS_Cpp11 LS_Auto
Standard: Cpp11
TabWidth: 4
#UT_Never UT_ForIndentation UT_Always
UseTab: Never
#SBPO_Never SBPO_ControlStatements SBPO_Always
